{
  "kind": "collectionType",
  "collectionName": "landingpage_webinars",
  "info": {
    "singularName": "landingpage-webinar",
    "pluralName": "landingpage-webinars",
    "displayName": "Landingpage-Webinar",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "Headline": {
      "type": "customField",
      "options": {
        "preset": "defaultHtml"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "Subtitle": {
      "type": "text"
    },
    "Benefits": {
      "type": "json"
    },
    "CtaButtonText": {
      "type": "string"
    },
    "SubButtonText": {
      "type": "text"
    },
    "Media": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "SubMediaText": {
      "type": "text"
    },
    "Content": {
      "type": "dynamiczone",
      "components": [
        "landingpage.lp-testimonial",
        "landingpage.lp-row-text",
        "landingpage.lp-row-text-image",
        "landingpage.lp-row-text-image-badge",
        "landingpage.lp-row-testimonials",
        "landingpage.lp-row-problemsection",
        "landingpage.lp-row-lawyers",
        "landingpage.lp-row-cta",
        "landingpage.lp-row-comparison-table",
        "landingpage.lp-row-benefits",
        "landingpage.lp-row-benefit-two-cols",
        "landingpage.benefit-items",
        "shared.faq"
      ]
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "CssGradient": {
      "type": "string"
    },
    "schedules": {
      "type": "json"
    },
    "urlWebinarRoom": {
      "type": "string"
    },
    "urlUmfrage": {
      "type": "string"
    },
    "calendarEventName": {
      "type": "string"
    },
    "calendarEventDescription": {
      "type": "text"
    },
    "shortPublicTitle": {
      "type": "string"
    },
    "hasBonus": {
      "type": "boolean"
    },
    "urlBonusSignup": {
      "type": "string"
    },
    "urlBestaetigungsseite": {
      "type": "string"
    },
    "urlCountdownseite": {
      "type": "string"
    },
    "kategorie": {
      "type": "enumeration",
      "enum": [
        "zoll"
      ],
      "required": true,
      "default": "zoll"
    },
    "conversionWebinarJamUrl": {
      "type": "string"
    }
  }
}
